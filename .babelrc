{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "add-module-exports",
    "@babel/plugin-proposal-class-properties",
    "css-modules-transform"
  ],
  "env": {
    "production": {
      "presets": [
        "@babel/preset-env",
        "minify"
      ],
      "plugins": [
        "add-module-exports"
      ]
    }
  }
}
